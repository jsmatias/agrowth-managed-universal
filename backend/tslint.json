{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-unused-variable": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "variable-name": [true, "allow-pascal-case"]
  },
  "rulesDirectory": []
}
